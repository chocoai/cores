<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Changeroom" table="changeroom" >
        <id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>

        <property name="monkeyid" type="java.lang.String">
            <column name="monkeyid"  length="20"/>
        </property>
        <property name="changeroomdate" type="java.sql.Timestamp">
            <column name="changeroomdate" />
        </property>
        <property name="changeinarea" type="java.lang.String">
            <column name="changeinarea"  length="20" />
        </property>
        <property name="changeinroom" type="java.lang.String">
            <column name="changeinroom"  length="20" />
        </property>
        <property name="protector" type="java.lang.String">
            <column name="protector"  length="20" />
        </property>
        <property name="recorder" type="java.lang.String">
            <column name="recorder"  length="20" />
        </property>
        <property name="operater" type="java.lang.String">
            <column name="operater"  length="20" />
        </property>
        <property name="createtime" type="java.sql.Timestamp">
            <column name="createtime"  />
        </property>
        <property name="modified_by" type="java.lang.Long">
            <column name="modified_by"  length="20" />
        </property>
        <property name="created_by" type="java.lang.Long">
            <column name="created_by"  length="20" />
        </property>
        <property name="lastmodifytime" type="java.sql.Timestamp">
            <column name="lastmodifytime" />
        </property>
        <property name="deleted" type="java.lang.Integer">
            <column name="deleted"  length="4" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"   />
        </property>
        <property name="lhao" type="java.lang.String">
            <column name="lhao"  length="20" />
        </property>
       <property name="yarea" type="java.lang.Long">
            <column name="yarea"  length="20" />
        </property>
        <property name="yroom" type="java.lang.Long">
            <column name="yroom"  length="20" />
        </property>
        <property name="ykeeper" type="java.lang.Long">
            <column name="ykeeper"  length="20" />
        </property>
        <property name="ylh" type="java.lang.String">
            <column name="ylh"  length="20" />
        </property>
        <property name="pccode" type="java.lang.String">
            <column name="pccode"  length="20" />
        </property>
        <property name="process" type="java.lang.String">
            <column name="process"  length="20" />
        </property>
    </class>
</hibernate-mapping>

package com.lanen.model;

import java.io.Serializable;

public class Security_ru implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -5742334528434937564L;
    private int id;        //id
    private int role_id;   //角色id
    private int unit_id;   //模块id
    private int permission;//许可
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getRole_id() {
		return role_id;
	}
	public void setRole_id(int roleId) {
		role_id = roleId;
	}
	public int getUnit_id() {
		return unit_id;
	}
	public void setUnit_id(int unitId) {
		unit_id = unitId;
	}
	public int getPermission() {
		return permission;
	}
	public void setPermission(int permission) {
		this.permission = permission;
	}
    
}

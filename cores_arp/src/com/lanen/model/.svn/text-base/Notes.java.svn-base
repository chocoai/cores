package com.lanen.model;

import java.io.Serializable;
import java.util.Date;

public class Notes implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 2282912950603847026L;
    private int id;                 //通知表id
    private String notes;           //通知内容
    private int created_by;         //发送者
    private Date createtime;        //发送时间
    private int deleted;            //删除标志
    private String showtype;        //展示类型
    private Date notedate;          //通知日期
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNotes() {
		return notes;
	}
	public void setNotes(String notes) {
		this.notes = notes;
	}
	public int getCreated_by() {
		return created_by;
	}
	public void setCreated_by(int createdBy) {
		created_by = createdBy;
	}
	public Date getCreatetime() {
		return createtime;
	}
	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}
	public int getDeleted() {
		return deleted;
	}
	public void setDeleted(int deleted) {
		this.deleted = deleted;
	}
	public String getShowtype() {
		return showtype;
	}
	public void setShowtype(String showtype) {
		this.showtype = showtype;
	}
	public Date getNotedate() {
		return notedate;
	}
	public void setNotedate(Date notedate) {
		this.notedate = notedate;
	}
    
}

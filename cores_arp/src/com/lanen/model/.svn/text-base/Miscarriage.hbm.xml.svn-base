<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Miscarriage" table="miscarriage" >
        <id name="id" column="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
        <property name="monkeyid" type="java.lang.String">
            <column name="monkeyid" length="20" />
        </property>
        <property name="miscarriagedate" type="java.util.Date">
            <column name="miscarriagedate"  />
        </property>
         <property name="remarks" type="java.lang.String">
            <column name="remarks" length="20" />
        </property>
        <property name="veterinarian" type="java.lang.Integer">
            <column name="veterinarian" length="20" />
        </property>
        
        <property name="protector" type="java.lang.Integer">
            <column name="protector" length="20"  />
        </property>
        
         <property name="recorder" type="java.lang.Integer">
            <column name="recorder" length="20"  />
        </property>
        <property name="operater" type="java.lang.Integer">
            <column name="operater" length="20"  />
        </property>
         <property name="deleted" type="java.lang.Integer">
            <column name="deleted"   />
        </property>
        <property name="modified_by" type="java.lang.Integer">
            <column name="modified_by"  length="20" />
        </property>
        <property name="createtime" type="timestamp">
            <column name="createtime"   />
        </property>
        
        <property name="created_by" type="java.lang.Integer">
            <column name="created_by" length="20"  />
        </property>
        <property name="lastmodifytime" type="timestamp">
            <column name="lastmodifytime"   />
        </property>
    </class>
</hibernate-mapping>

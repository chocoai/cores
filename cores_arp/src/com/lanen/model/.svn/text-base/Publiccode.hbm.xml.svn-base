<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Publiccode" table="publiccode" >
        <id name="id" column="id" type="java.lang.Integer">
			<generator class="native" />
		</id>

        <property name="name" type="java.lang.String">
            <column name="name"  />
        </property>
        <property name="mark" type="java.lang.String">
            <column name="mark" />
        </property>
        <property name="desciption" type="java.lang.String">
            <column name="desciption"  />
        </property>
        <property name="ref_id" type="java.lang.Integer">
            <column name="ref_id" length="11"/>
        </property>
    </class>
</hibernate-mapping>

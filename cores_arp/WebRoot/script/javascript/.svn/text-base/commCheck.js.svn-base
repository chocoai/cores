/**
 * 类型检查(是否是数字)
 * @param input
 * @return
 */
function numberCheck(input){
	if(isNaN(input)){
		return true;
	} else{
		return false;
	}
}

/**
 * 类型检查(是否是正整数)
 * @param input
 * @return
 */
function fullNumCheck(input){
	if(/(^[1-9]*[1-9][0-9]*$)/.test(input)){
		return true;
	} else{
		return false;
	}
}
function isSignlessInteger(input){
	if(/(^[1-9]*[1-9][0-9]*$)/.test(input)){
		return true;
	} else{
		return false;
	}
}

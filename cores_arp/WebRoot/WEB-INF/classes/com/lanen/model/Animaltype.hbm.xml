<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Animaltype" table="animaltype" >
        <id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
         <property name="desciption" type="java.lang.String">
            <column name="desciption" length="255"  />
        </property>
         <property name="del" type="java.lang.Integer">
            <column name="del" />
        </property>
    </class>
</hibernate-mapping>

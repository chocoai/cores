<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Tb" table="tb" >
        <id name="id" column="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
        <property name="monkeyid" type="java.lang.String">
            <column name="monkeyid" length="20" />
        </property>
        <property name="protector" type="java.lang.String">
            <column name="protector" length="20"  />
        </property>
         <property name="recorder" type="java.lang.String">
            <column name="recorder" length="20"/>
        </property>
        <property name="createtime" type="timestamp">
            <column name="createtime"  />
        </property>
        
        <property name="modified_by" type="java.lang.Integer">
            <column name="modified_by" length="20"  />
        </property>
        
         <property name="created_by" type="java.lang.Integer">
            <column name="created_by" length="20"  />
        </property>
        <property name="lastmodifytime" type="timestamp">
            <column name="lastmodifytime" />
        </property>
         <property name="deleted" type="java.lang.Byte">
            <column name="deleted" length="4"  />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"  />
        </property>
        <property name="veterinarian" type="java.lang.String">
            <column name="veterinarian"  length="20" />
        </property>
        
        <property name="normal_id" type="java.lang.Integer">
            <column name="normal_id" length="20"  />
        </property>
        <property name="q_id" type="java.lang.String">
            <column name="q_id" length="20"  />
        </property>
        
        
        <property name="drugs_count" type="java.lang.String">
            <column name="drugs_count" length="20"  />
        </property>
        
        
        <property name="cdate" type="java.util.Date">
            <column name="cdate"  />
        </property>
        <property name="ptype" type="java.lang.String">
            <column name="ptype" length="50"  />
        </property>
        <property name="dateid" type="java.lang.String">
            <column name="dateid" length="50"  />
        </property>
        
        
         <property name="process" type="java.lang.String">
            <column name="process"  length="20"/>
        </property>
         <property name="pccode" type="java.lang.String">
            <column name="pccode" length="20"  />
        </property> 
        
        <property name="tb24" type="java.lang.String">
            <column name="tb24" length="20"  />
        </property>
        <property name="tb24v" type="java.lang.String">
            <column name="tb24v" length="20"  />
        </property>
        <property name="tb48" type="java.lang.String">
            <column name="tb48" length="20"  />
        </property>
        <property name="tb48v" type="java.lang.String">
            <column name="tb48v" length="20"  />
        </property>
        <property name="tb72" type="java.lang.String">
            <column name="tb72" length="20"  />
        </property>
        <property name="tb72v" type="java.lang.String">
            <column name="tb72v" length="20"  />
        </property>
  
    </class>
</hibernate-mapping>

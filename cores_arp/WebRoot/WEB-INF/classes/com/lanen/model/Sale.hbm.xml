<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Sale" table="sale" >
        <id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>

        <property name="outdate" type="java.sql.Timestamp">
            <column name="outdate"  />
        </property>
        <property name="approveserial" type="java.lang.Long">
            <column name="approveserial" />
        </property>
        <property name="saledate" type="java.sql.Timestamp">
            <column name="saledate"  length="20" />
        </property>
        <property name="cites" type="java.lang.String">
            <column name="cites"  length="20" />
        </property>
        <property name="carriagenumber" type="java.lang.Long">
            <column name="carriagenumber"  length="20" />
        </property>
        <property name="saleaddress_id" type="java.lang.Long">
            <column name="saleaddress_id"  length="20" />
        </property>
        <property name="saletype" type="java.lang.Long">
            <column name="saletype"  length="20" />
        </property>
        <property name="boss" type="java.lang.Long">
            <column name="boss" length="20"  />
        </property>
        <property name="protector" type="java.lang.Long">
            <column name="protector" length="20"  />
        </property>
        <property name="recorder" type="java.lang.Long">
            <column name="recorder" length="20"  />
        </property>
        <property name="operater" type="java.lang.Long">
            <column name="operater" length="20"  />
        </property>
        <property name="monkeylist" type="java.lang.String">
            <column name="monkeylist"  />
        </property>
        <property name="createtime" type="java.sql.Timestamp">
            <column name="createtime" />
        </property>
        <property name="modified_by" type="java.lang.Long">
            <column name="modified_by"  length="20" />
        </property>
        <property name="created_by" type="java.lang.Long">
            <column name="created_by"  length="20" />
        </property>
        <property name="lastmodifytime" type="java.sql.Timestamp">
            <column name="lastmodifytime" />
        </property>
        <property name="deleted" type="java.lang.Integer">
            <column name="deleted"  length="4" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"  length="20" />
        </property>
        <property name="export" type="java.lang.Long">
            <column name="export"  length="20" />
        </property>
        <property name="trance" type="java.lang.Long">
            <column name="trance"  length="20" />
        </property>
        <property name="tiaojian" type="java.lang.String">
            <column name="tiaojian"  length="255" />
        </property>
        <property name="monkeytype" type="java.lang.Long">
            <column name="monkeytype"  length="20" />
        </property>
        <property name="salecount" type="java.lang.String">
            <column name="salecount"  length="20" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"   />
        </property>
        <property name="lyjpw" type="java.lang.Long">
            <column name="lyjpw"  length="20" />
        </property>
        <property name="saleout" type="java.lang.Integer">
            <column name="saleout"  length="4" />
        </property>
        <property name="saleoutremark" type="java.lang.String">
            <column name="saleoutremark"   />
        </property>
        <property name="title" type="java.lang.String">
            <column name="title"  length="255" />
        </property>
    </class>
</hibernate-mapping>

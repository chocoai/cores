<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Salemonkey" table="salemonkey" >
        <id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>
        
        <property name="sale_id" type="java.lang.Long">
            <column name="sale_id" />
        </property>
        <property name="monkeyid" type="java.lang.String">
            <column name="monkeyid"  length="20"/>
        </property>
        
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Notes" table="notes" >
        <id name="id" column="id" type="java.lang.Integer">
			<generator class="native" />
		</id>

        <property name="notes" type="java.lang.String">
            <column name="notes"  />
        </property>
        <property name="created_by" type="java.lang.Integer">
            <column name="created_by" />
        </property>
        <property name="createtime" type="java.sql.Timestamp">
            <column name="createtime"   />
        </property>
        <property name="deleted" type="java.lang.Integer">
            <column name="deleted"  length="4" />
        </property>
        <property name="showtype" type="java.lang.String">
            <column name="showtype"  length="20" />
        </property>
        <property name="notedate" type="java.sql.Timestamp">
            <column name="notedate"   />
        </property>
        
       
        
        
    </class>
</hibernate-mapping>

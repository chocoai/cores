<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Approval" table="approval" >
        <id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>

        <property name="title" type="java.lang.String">
            <column name="title"  />
        </property>
        <property name="phao" type="java.lang.String">
            <column name="phao" length="100" />
        </property>
        <property name="head" type="java.lang.String">
            <column name="head"  length="100" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content"   />
        </property>
        
        <property name="createtime" type="java.sql.Timestamp">
            <column name="createtime" />
        </property>
        <property name="modified_by" type="java.lang.Long">
            <column name="modified_by"  length="20" />
        </property>
        <property name="created_by" type="java.lang.Long">
            <column name="created_by"  length="20" />
        </property>
        <property name="lastmodifytime" type="java.sql.Timestamp">
            <column name="lastmodifytime" />
        </property>
        <property name="deleted" type="java.lang.Integer">
            <column name="deleted"  length="4" />
        </property>
        <property name="approvaldate" type="java.sql.Timestamp">
            <column name="approvaldate"  length="20" />
        </property>
        <property name="pic" type="java.lang.String">
            <column name="pic"  />
        </property>
    </class>
</hibernate-mapping>

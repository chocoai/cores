<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Childbirth" table="childbirth" >
        <id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>

        <property name="monkeyid" type="java.lang.String">
            <column name="monkeyid"  length="20"/>
        </property>
        <property name="monkeyids" type="java.lang.String">
            <column name="monkeyids"  length="200"/>
        </property>
        <property name="labordate" type="java.lang.String">
            <column name="labordate" length="20"/>
        </property>
        <property name="childercount" type="java.lang.Integer">
            <column name="childercount"  length="4" />
        </property>
        <property name="laborcondition" type="java.lang.String">
            <column name="laborcondition"   />
        </property>
        <property name="veterinarian" type="java.lang.Long">
            <column name="veterinarian"  length="20" />
        </property>
        <property name="keeper" type="java.lang.Long">
            <column name="keeper"  length="20" />
        </property>
        <property name="protector" type="java.lang.Long">
            <column name="protector"  length="20" />
        </property>
        <property name="recorder" type="java.lang.Long">
            <column name="recorder"  length="20" />
        </property>
        <property name="operater" type="java.lang.Long">
            <column name="operater"  length="20" />
        </property>
        <property name="createtime" type="java.sql.Timestamp">
            <column name="createtime"  />
        </property>
        <property name="modified_by" type="java.lang.Long">
            <column name="modified_by"  length="20" />
        </property>
        <property name="created_by" type="java.lang.Long">
            <column name="created_by"  length="20" />
        </property>
        <property name="lastmodifytime" type="java.sql.Timestamp">
            <column name="lastmodifytime" />
        </property>
        <property name="deleted" type="java.lang.Integer">
            <column name="deleted"  length="4" />
        </property>
        <property name="leavebreastdate" type="java.lang.String">
            <column name="leavebreastdate"  length="20" />
        </property>
        <property name="leavebreastweight" type="java.lang.String">
            <column name="leavebreastweight"  length="20" />
        </property>
        <property name="room" type="java.lang.String">
            <column name="room"  length="20" />
        </property>
        <property name="createin" type="java.lang.String">
            <column name="createin"  length="20" />
        </property>
        
    </class>
</hibernate-mapping>

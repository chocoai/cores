<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.Xcg" table="xcg" >
        <id name="id" column="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
        <property name="monkeyid" type="java.lang.String">
            <column name="monkeyid" length="20" />
        </property>
        
         <property name="recorder" type="java.lang.String">
            <column name="recorder" length="50"/>
        </property>
        <property name="createtime" type="timestamp">
            <column name="createtime"  />
        </property>
        
        <property name="modified_by" type="java.lang.Integer">
            <column name="modified_by" length="20"  />
        </property>
        
         <property name="created_by" type="java.lang.Integer">
            <column name="created_by" length="20"  />
        </property>
        <property name="lastmodifytime" type="timestamp">
            <column name="lastmodifytime" />
        </property>
         <property name="deleted" type="java.lang.Byte">
            <column name="deleted" length="4"  />
        </property>
        
        <property name="veterinarian" type="java.lang.String">
            <column name="veterinarian"  length="20" />
        </property>
        
        <property name="normal_id" type="java.lang.Integer">
            <column name="normal_id" length="20"  />
        </property>
        
        <property name="cdate" type="java.util.Date">
            <column name="cdate"  />
        </property>
        <property name="ptype" type="java.lang.String">
            <column name="ptype" length="50"  />
        </property>
        <property name="dateid" type="java.lang.String">
            <column name="dateid" length="50"  />
        </property>
        
        <property name="wbc" type="java.lang.String">
            <column name="wbc" length="255"  />
        </property>
        <property name="rbc" type="java.lang.String">
            <column name="rbc" length="255"  />
        </property>
        <property name="hgb" type="java.lang.String">
            <column name="hgb" length="255"  />
        </property>
        <property name="hct" type="java.lang.String">
            <column name="hct" length="255"  />
        </property>
        <property name="plt" type="java.lang.String">
            <column name="plt" length="255"  />
        </property>
        <property name="mcv" type="java.lang.String">
            <column name="mcv" length="255"  />
        </property>
        <property name="mch" type="java.lang.String">
            <column name="mch" length="255"  />
        </property>
        <property name="mchc" type="java.lang.String">
            <column name="mchc" length="255"  />
        </property>
        <property name="lym" type="java.lang.String">
            <column name="lym" length="255"  />
        </property>
        <property name="mid" type="java.lang.String">
            <column name="mid" length="255"  />
        </property>
        <property name="gra" type="java.lang.String">
            <column name="gra" length="255"  />
        </property>
        <property name="bhao" type="java.lang.String">
            <column name="bhao" length="20"  />
        </property>
         <property name="process" type="java.lang.String">
            <column name="process"  length="20"/>
        </property>
         <property name="pccode" type="java.lang.String">
            <column name="pccode" length="20"  />
        </property>   

    </class>
</hibernate-mapping>

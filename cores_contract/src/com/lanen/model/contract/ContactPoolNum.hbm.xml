<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.contract">
    <class name="com.lanen.model.contract.ContractPoolNum" table="contractPoolNum" >
        <id name="id" type="long">
            <column name="id"  />
        </id>
        <property name="currentYear" type="java.lang.String">
            <column name="currentYear"  length="20"/>
        </property>
        
         <property name="prefix" type="java.lang.String">
            <column name="prefix"  length="20" />
        </property>
        
         <property name="serialNumLen" type="integer">
            <column name="serialNumLen"   />
        </property>
        
        <property name="currentSerialNum" type="java.lang.String">
            <column name="currentSerialNum"  length="20"/>
        </property>
        
         <property name="kind" type="integer">
            <column name="kind"   />
        </property>
       
    </class>
</hibernate-mapping>

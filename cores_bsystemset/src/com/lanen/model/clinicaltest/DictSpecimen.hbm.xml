<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.clinicaltest">
    <class name="com.lanen.model.clinicaltest.DictSpecimen" table="dictSpecimen" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="specKind" type="java.lang.String">
            <column name="specKind"  length="20"/>
        </property>
        <property name="specType" type="java.lang.Integer">
            <column name="specType" />
        </property>
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.DictVisceraAnimal" table="dictVisceraAnimal" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>    
        <property name="visceraCode" type="java.lang.String">
            <column name="visceraCode"  length="20" />
        </property>
        <property name="animalTypeName" type="java.lang.String">
            <column name="animalTypeName"  length="60"/>
        </property>
       
    </class>
</hibernate-mapping>

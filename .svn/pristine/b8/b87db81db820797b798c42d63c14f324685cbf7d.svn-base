<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.schedule">
    <class name="com.lanen.model.schedule.TblStudyInfo" table="tblStudyInfo" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>    
        <property name="studyNo" type="java.lang.String">
            <column name="studyNo" length="20" />
        </property>
        <property name="resID" type="java.lang.String">
            <column name="resID"  />
        </property>
        <property name="scheduleReviewSignID" type="java.lang.String">
            <column name="scheduleReviewSignID"  length="20"/>
        </property>

    </class>
</hibernate-mapping>

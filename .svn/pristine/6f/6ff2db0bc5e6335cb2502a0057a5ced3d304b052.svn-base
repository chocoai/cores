<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.contract">
    <class name="com.lanen.model.contract.TblStudyScheduleNode" table="tblStudyScheduleNode" >
        <id name="id" type="java.lang.String">
            <column name="id" length="20"/>
        </id>
        <property name="studyTypeCode" type="java.lang.String">
            <column name="studyTypeCode"  length="50" />
        </property>
        <property name="nodeSn" type="java.lang.Integer">
            <column name="nodeSn"  />
        </property>
        <property name="nodeName" type="java.lang.String">
            <column name="nodeName"  length="100" />
        </property>
        <property name="planDays" type="java.lang.String">
            <column name="planDays" length="20" />
        </property>
        <property name="defaultNode" type="java.lang.Integer">
            <column name="defaultNode"  />
        </property>
        
    </class>
</hibernate-mapping>

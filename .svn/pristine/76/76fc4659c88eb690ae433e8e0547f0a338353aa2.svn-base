<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.studyplan">
    <class name="com.lanen.model.studyplan.DictBloodCoag" table="dictBloodCoag" >
        <id name="name" type="java.lang.String">
            <column name="name"  length="50"/>
        </id>
        <property name="abbr" type="java.lang.String">
            <column name="abbr"  length="20"/>
        </property>
        <property name="precision" type="integer">
            <column name="precision1" />
        </property>
        <property name="unit" type="java.lang.String">
            <column name="unit"  length="20"/>
        </property>
        <property name="orderNo" type="java.lang.Integer">
            <column name="orderNo" />
        </property>
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.schedule">
    <class name="com.lanen.model.schedule.TblAnimalHouse" table="tblAnimalHouse" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>    
        <property name="resKind" type="java.lang.Integer">
            <column name="resKind"  />
        </property>
        <property name="resName" type="java.lang.String">
            <column name="resName"  length="60"/>
        </property>
         <property name="validFlag" type="java.lang.String">
            <column name="validFlag"  length="20"/>
        </property>
        <property name="parentId" type="java.lang.String">
            <column name="parentId"  length="20"/>
        </property>
        <property name="orderNo" type="java.lang.Integer">
            <column name="orderNo"  />
        </property>
       
       
    </class>
</hibernate-mapping>

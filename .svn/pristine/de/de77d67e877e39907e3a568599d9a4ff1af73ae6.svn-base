<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblTissueSliceIndex" table="tblTissueSliceIndex" >
       <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="studyNo" type="java.lang.String">
            <column name="studyNo" length="20" />
        </property>
        <property name="sliceCodeType" type="java.lang.Integer">
            <column name="sliceCodeType" />
        </property>
		<property name="operatorSign" type="java.lang.String">
            <column name="operatorSign" length="20" />
        </property>
		<property name="createTime" type="timestamp">
            <column name="createTime"/>
        </property>
    </class>
</hibernate-mapping>

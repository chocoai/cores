<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.studyplan">
    <class name="com.lanen.model.studyplan.DictAnimalStrain" table="dictAnimalStrain" >
        <id name="id" type="java.lang.String">
            <column name="id" length="20"/>
        </id>
        <property name="animalTypeId" type="java.lang.String">
            <column name="animalTypeId"  length="20"/>
        </property>
        <property name="strainName" type="java.lang.String">
            <column name="strainName"  length="50"/>
        </property>
        <property name="orderNo" type="java.lang.String">
            <column name="orderNo"  length="10" />
        </property>
    </class>
</hibernate-mapping>

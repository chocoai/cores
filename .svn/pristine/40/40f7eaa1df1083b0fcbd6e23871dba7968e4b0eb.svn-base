<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblSuperficialTumorViscera" table="tblSuperficialTumorViscera" >
        <id name="visceraCode" type="java.lang.String">
            <column name="visceraCode"  length="20"/>

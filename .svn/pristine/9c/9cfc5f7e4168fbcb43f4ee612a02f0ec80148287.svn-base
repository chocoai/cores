<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model">
    <class name="com.lanen.model.VersionControl" table="versionControl" >
        <id name="systemName" type="java.lang.String">
            <column name="systemName"  length="200"/>
        </id>
        <property name="leastVersion" type="java.lang.String">
            <column name="leastVersion"  length="20"/>
        </property>
        
    </class>
</hibernate-mapping>

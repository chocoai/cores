package com.lanen.model.clinicaltest;

/**
 * 检验编号池
 * @author 黄国刚
 *
 */
public class PoolSpecimenCode implements java.io.Serializable{
	
	private static final long serialVersionUID = 7048238252588884263L;
	
	private String id;				//id
	private String studyNo;			//专题编号
	private int reqNo;				//申请编号
	private int testItem;          	//检验项目
	private String createDate  ;   	//yyyy-MM-dd   产生日期
	private String specimenCode;	//检验编号
	private String animalId;        //动物Id号
	private int flag; 				// 是否使用    0：未用    1：使用      
	
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getStudyNo() {
		return studyNo;
	}
	public void setStudyNo(String studyNo) {
		this.studyNo = studyNo;
	}
	public int getReqNo() {
		return reqNo;
	}
	public void setReqNo(int reqNo) {
		this.reqNo = reqNo;
	}
	public int getTestItem() {
		return testItem;
	}
	public void setTestItem(int testItem) {
		this.testItem = testItem;
	}
	public String getCreateDate() {
		return createDate;
	}
	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}
	public String getSpecimenCode() {
		return specimenCode;
	}
	public void setSpecimenCode(String specimenCode) {
		this.specimenCode = specimenCode;
	}
	public String getAnimalId() {
		return animalId;
	}
	public void setAnimalId(String animalId) {
		this.animalId = animalId;
	}
	public int getFlag() {
		return flag;
	}
	public void setFlag(int flag) {
		this.flag = flag;
	}
	
	
}

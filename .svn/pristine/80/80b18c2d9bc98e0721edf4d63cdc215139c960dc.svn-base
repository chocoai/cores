<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.schedule">
    <class name="com.lanen.model.schedule.TblTaskType" table="tblTaskType" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>    
        
        <property name="taskName" type="java.lang.String">
            <column name="taskName"  length="60"/>
        </property>
        
        <property name="taskKind" type="java.lang.Integer">
            <column name="taskKind"  />
        </property>  
        <property name="validFlag" type="java.lang.String">
            <column name="validFlag"  length="20"/>
        </property>
    </class>
</hibernate-mapping>

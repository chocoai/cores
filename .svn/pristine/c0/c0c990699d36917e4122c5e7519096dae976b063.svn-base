<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.studyplan">
    <class name="com.lanen.model.studyplan.TblDissectPlanVersion" table="tblDissectPlanVersion" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="dissectNum" type="java.lang.Integer">
            <column name="dissectNum" />
        </property>
        <property name="beginDate" type="timestamp">
            <column name="beginDate" />
        </property>
        <property name="endDate" type="timestamp">
            <column name="endDate" />
        </property>
        <property name="version" type="integer">
            <column name="version" />
        </property>
        <property name="studyNo" type="java.lang.String">
            <column name="studyNo"  length="20"/>
        </property>
    </class>
</hibernate-mapping>

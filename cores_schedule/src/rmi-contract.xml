   </bean>
    <bean id="TblContractService" class="org.springframework.remoting.rmi.RmiProxyFactoryBean"> 
       <property name="serviceUrl" value="rmi://${userprivilegeIP}:6688/TblContractService"/> 
       <property name="serviceInterface">

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblAnimalTargetOrgan" table="tblAnimalTargetOrgan" >
       <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="studyNo" type="java.lang.String">
            <column name="studyNo" length="20" />
        </property>
        <property name="gender" type="java.lang.Integer">
            <column name="gender"/>
        </property>       
        <property name="visceraType" type="java.lang.Integer">
            <column name="visceraType"/>
        </property>       
        <property name="visceraCode" type="java.lang.String">
            <column name="visceraCode" length="20" />
        </property>
        <property name="visceraName" type="java.lang.String">
            <column name="visceraName" length="20" />
        </property>
        <property name="occurPhase" type="java.lang.String">
            <column name="occurPhase" length="60" />
        </property>
        <property name="targetOrganFlag" type="java.lang.Integer">
            <column name="targetOrganFlag"/>
        </property>       
        <property name="regDate" type="java.sql.Timestamp">
            <column name="regDate"  />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="200" />
        </property>
        <property name="signId" type="java.lang.String">
            <column name="signId" length="20" />
        </property> 
        <property name="delFlag" type="java.lang.Integer">
            <column name="delFlag"/>
        </property>
        <property name="delRsn" type="java.lang.String">
            <column name="delRsn" length="200" />
        </property>
        <property name="delTime" type="java.sql.Timestamp">
            <column name="delTime"/>
        </property>
        <property name="delSignId" type="java.lang.String">
            <column name="delSignId" length="20"/>
        </property>
        
    </class>
</hibernate-mapping>

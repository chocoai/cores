<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblBalConnect" table="tblBalConnect" >
       <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="hostName" type="java.lang.String">
            <column name="hostName" length="100" />
        </property>
		<property name="balCode" type="java.lang.String">
            <column name="balCode" length="60" />
        </property>
		<property name="commName" type="java.lang.String">
            <column name="commName" length="10" />
        </property>
        <property name="enabled" type="java.lang.Integer">
            <column name="enabled" />
        </property>
         <property name="type" type="java.lang.Integer">
            <column name="type" />
        </property>
       
    </class>
</hibernate-mapping>

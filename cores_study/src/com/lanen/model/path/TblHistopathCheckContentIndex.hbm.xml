<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblHistopathCheckContentIndex" table="tblHistopathCheckContentIndex" >
       <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="studyNo" type="java.lang.String">
            <column name="studyNo" length="20" />
        </property>
		 <property name="contentName" type="java.lang.String">
            <column name="contentName" length="200" />
        </property>
        
        <property name="createDate" type="java.sql.Timestamp">
            <column name="createDate" />
        </property>
       
		<property name="creator" type="java.lang.String">
            <column name="creator" length="20" />
        </property>
    </class>
</hibernate-mapping>

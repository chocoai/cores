<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblTissueSliceBatchAnimal" table="tblTissueSliceBatchAnimal" >
       <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="batchId" type="java.lang.String">
            <column name="batchId" length="20" />
        </property>
        <property name="batchSn" type="java.lang.Integer">
            <column name="batchSn" />
        </property>
        <property name="animalCode" type="java.lang.String">
            <column name="animalCode" length="20"/>
        </property>       
    </class>
</hibernate-mapping>

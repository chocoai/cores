<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.path">
    <class name="com.lanen.model.path.TblBalCalibrationIndex" table="tblBalCalibrationIndex" >
       <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="hostCode" type="java.lang.String">
            <column name="hostCode" length="20" />
        </property>
		 <property name="balCode" type="java.lang.String">
            <column name="balCode" length="20" />
        </property>
		<property name="calBeginTime" type="java.sql.Timestamp">
            <column name="calBeginTime" />
        </property>
        <property name="calEndTime" type="java.sql.Timestamp">
            <column name="calEndTime" />
        </property>
        <property name="calResult" type="java.lang.Integer">
            <column name="calResult" />
        </property>
		 <property name="calSign" type="java.lang.String">
            <column name="calSign" length="20" />
        </property>
        
    </class>
</hibernate-mapping>

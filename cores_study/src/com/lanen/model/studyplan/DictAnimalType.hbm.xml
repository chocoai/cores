<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.studyplan">
    <class name="com.lanen.model.studyplan.DictAnimalType" table="dictAnimalType" >
        <id name="id" type="java.lang.String">
            <column name="id" length="20"/>
        </id>
        <property name="typeName" type="java.lang.String">
            <column name="typeName"  length="50"/>
        </property>
        <property name="abbr" type="java.lang.String">
            <column name="abbr"  length="20"/>
        </property>
        <property name="orderNo" type="java.lang.String">
            <column name="orderNo"  length="10"/>
        </property>
        <property name="isBigAnimal" type="java.lang.Integer">
            <column name="isBigAnimal" />
        </property>
    </class>
</hibernate-mapping>

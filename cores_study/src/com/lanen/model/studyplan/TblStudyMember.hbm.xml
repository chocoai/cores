<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lanen.model.studyplan">
    <class name="com.lanen.model.studyplan.TblStudyMember" table="tblStudyMember" >
        <id name="id" type="java.lang.String">
            <column name="id"  length="20"/>
        </id>
        <property name="StudyNo" type="java.lang.String">
            <column name="StudyNo"  length="20"/>
        </property>
        <property name="Member" type="java.lang.String">
            <column name="Member"  length="200"/>
        </property>
    </class>
</hibernate-mapping>
